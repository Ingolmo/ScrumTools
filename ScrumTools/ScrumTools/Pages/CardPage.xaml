<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ScrumTools.Pages.CardPage"
             Title="Estimation">
    <ContentPage.Content>
        <Frame Padding="0" Margin="5" VerticalOptions="Center" HorizontalOptions="Center">
            <Image x:Name="image" Source="{Binding Image}"/>
        </Frame>
    </ContentPage.Content>
</ContentPage>